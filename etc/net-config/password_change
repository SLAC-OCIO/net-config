[DEFAULT]
password_dir = /afs/slac.stanford.edu/g/scs/net/cisco/config/etc



####

[TEMPLATE]

# current passwords
login_password = %(password_dir)s/login.password
enable_password = %(password_dir)s/enable.password

# new passwords
new_login_password = %(password_dir)s/login.password.new
new_enable_password = %(password_dir)s/enable.password.new

# place to move the current passwords once done
old_login_password = %(password_dir)s/login.password.old
old_enable_password = %(password_dir)s/enable.password.old

# snmp stuff
snmp_read_password = %(password_dir)s/snmp.community.read
snmp_write_password = %(password_dir)s/snmp.community.write

new_snmp_read_password = %(password_dir)s/snmp.community.read.new
new_snmp_write_password = %(password_dir)s/snmp.community.write.new

old_snmp_read_password = %(password_dir)s/snmp.community.read.old
old_snmp_write_password = %(password_dir)s/snmp.community.write.old

# user accounts
# deal with these as user_<name>_<password type>

user_configuration_login_password = %(password_dir)s/configuration.password
user_configuration_level = 15

user_configuration_new_login_password = %(password_dir)s/configuration.password.new
user_configuration_old_login_password = %(password_dir)s/configuration.password.new






[STANDARD]

# where to OLD the password files once done
CURRENT_PASSWORD=$PASSWORD_DIR/login.password
CURRENT_ENABLE_PASSWORD=$PASSWORD_DIR/enable.password

CURRENT_SNMP_READ_PASSWORD=$PASSWORD_DIR/snmp.community.read
CURRENT_SNMP_WRITE_PASSWORD=$PASSWORD_DIR/snmp.community.write

CURRENT_CONFIGURATION_PASSWORD=$PASSWORD_DIR/configuration.password


# what the new password are
NEW_PASSWORD=$PASSWORD_DIR/login.password.new
NEW_ENABLE_PASSWORD=$PASSWORD_DIR/enable.password.new

NEW_SNMP_READ_PASSWORD=$PASSWORD_DIR/snmp.community.read.new
NEW_SNMP_WRITE_PASSWORD=$PASSWORD_DIR/snmp.community.write.new

NEW_CONFIGURATION_PASSWORD=$PASSWORD_DIR/configuration.password.new

# where to store the old pass word
OLD_PASSWORD=$PASSWORD_DIR/login.password.new
OLD_ENABLE_PASSWORD=$PASSWORD_DIR/enable.password.new

OLD_SNMP_READ_PASSWORD=$PASSWORD_DIR/snmp.community.read.new
OLD_SNMP_WRITE_PASSWORD=$PASSWORD_DIR/snmp.community.write.new

OLD_CONFIGURATION_PASSWORD=$PASSWORD_DIR/configuration.password.new


[WIRELESS]

user=netdev
# where to OLD the password files once done
login_password=$PASSWORD_DIR/login.password

snmp_read_password=$PASSWORD_DIR/snmp.community.read
snmp_write_password=$PASSWORD_DIR/snmp.community.write

configuration_password=$PASSWORD_DIR/configuration.password


# what the new password are
